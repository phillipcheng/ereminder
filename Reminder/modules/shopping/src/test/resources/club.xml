							</pageVerify>
						</value>
					</successNextPage>
				</nextpage>
			</link>
		</firstPageClickStream>
	</PrdTask>
	<PrdTask>
		<baseBrowseTask enableJS="true" taskName="prd">
			<param name="detailPages" type="list"/>
			<startUrl value="detailPages" fromType="expression"/>
