<Tasks xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shopping/src/main/resources/Task_2_0.xsd" rootVolume="999990" storeId="sina-stock-earnnotice" productType="default">
	<PrdTask>
		<baseBrowseTask enableJS="true" taskName="earnnotice" dsm="hdfs">
			<param name="stockid"/>
			<param name="startDate" type="string"/>
			<param name="endDate" type="string"/>
			<param name="HasHeader" type="boolean" value="false"/>
			<param name="ColNum" type="int" value="1"/>
			<param name="ColDateIdx" type="int" value="0"/><!-- 公告日期 -->
			<param name="RowNum" type="int" value="6"/>
			<param name="DataType" value="Text"/>
			<startUrl value="http://money.finance.sina.com.cn/corp/go.php/vFD_AchievementNotice/stockid/[stockid].phtml"/>
			<userAttribute name="id">
				<value value="stockid" fromScope="param"/>
			</userAttribute>
			<userAttribute name="data">
				<value value="//table[@id='Table1']/tbody/tr[count(td)=2]/td[2]" toType="list"/>
			</userAttribute>
			<csvtransform transformClass="org.cld.etl.csv.SpreadColTableAsCSV" outputType="byId"><!-- output #stockid files -->
				<outputDir value="'sina-stock-achievenotice/'+startDate+'_'+endDate" fromType="expression"/>
			</csvtransform>
		</baseBrowseTask>
	</PrdTask>
</Tasks>
