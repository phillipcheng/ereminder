<?xml version="1.0" encoding="UTF-8"?>
<Tasks xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shopping/src/main/resources/Task_2_0.xsd" 
	rootVolume="999990" 
	storeId="sina-stock-earnnotice" 
	productType="default">
	<PrdTask>
		<baseBrowseTask
		startUrl="http://money.finance.sina.com.cn/corp/go.php/vFD_AchievementNotice/stockid/[stockid].phtml" 
		enableJS="true" 
		taskName="earnnotice"
		dsm="hdfs">
			<param name="stockid"/>
			<param name="ColNum" type="int" value="6"/>
			<userAttribute name="id">
				<value value="stockid" fromScope="param"/>
			</userAttribute>
			<userAttribute name="data">
				<value value="//table[@id='Table1']/tbody/tr[count(td)=2]/td[2]" toType="list"/>
			</userAttribute>
			<csvtransform>org.cld.stock.sina.AchieveNoticeToCSV</csvtransform>
		</baseBrowseTask>
	</PrdTask>
</Tasks>
